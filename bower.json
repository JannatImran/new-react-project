{
  "name": "FP.Application",
  "version": "0.0.1",
  "main": "public/javascripts/app.js",
  "dependencies": {
    "html5shiv": "~3.7.0",
    "console-polyfill": "~0.1.0",
    "jquery": ">=2.0",
    "normalize-css": "~2.1.2",
    "h5bp-helpers": "~0.1.0",
    "bootstrap": "~3.1.0",
    "moment": "2.x",
    "react": "~0.10.0",
    "respond": "1.x",
    "es5-shim": "~2.1.0",
    "js-md5": "~1.1.0",
    "font-awesome": "~4.0.3",
    "ogre-js": "~0.1.3",
    "elucidata-type": "~1.1.1",
    "elucidata-react-coffee": "~0.5.0"
  },
  "overrides": {
    "es5-shim": {
      "dependencies": {
        "html5shiv": "*"
      },
      "main": [
        "es5-shim.js",
        "es5-sham.js"
      ]
    },
    "respond": {
      "main": "respond.min.js"
    },
    "react": {
      "dependencies": {
        "es5-shim": "*",
        "console-polyfill": "*",
        "respond": "*"
      },
      "main": "react-with-addons.js"
    },
    "bootstrap": {
      "main": [
        "dist/js/bootstrap.js",
        "dist/css/bootstrap.css",
        "dist/css/bootstrap-theme.css"
      ]
    },
    "font-awesome": {
      "main": [
        "css/font-awesome.css"
      ]
    },
    "elucidata-react-coffee": {
      "dependencies": {
        "react": "*"
      }
    }
  }
}
